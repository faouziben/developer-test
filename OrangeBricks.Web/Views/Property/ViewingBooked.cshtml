@model IEnumerable<OrangeBricks.Web.Controllers.Property.ViewModels.BookViewingViewModel>

@{
    ViewBag.Title = "ViewingBooked";
}

<h2>Viewings Booked</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PropertyType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StreetName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateV)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PropertyType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StreetName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateV)
        </td>
    </tr>
}

</table>
